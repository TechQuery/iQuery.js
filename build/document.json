{
    "source":          {
        "include":    ["./source/"]
    },
    "opts":            {
        "recurse":        true,
        "readme":         "ReadMe.md",
        "template":       "node_modules/docdash",
        "destination":    "doc/API/",
        "verbose":        true
    },
    "templates":       {
        "default":    {
            "includeDate":    false
        }
    },
    "plugins":         ["plugins/markdown", "node_modules/test-example"],
    "markdown":        {
        "hardwrap":    true
    },
    "test-example":    {
        "sourcePath":    "./source/",
        "overWrite":     true,
        "headerFile":    "./build/TestHeader.js",
        "hookModule":    "./build/TestHook"
    }
}
